{
    "name": "schooltry/ai-documentation-generator",
    "description": "AI-powered documentation generator for Laravel applications with multi-provider support",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "SchoolTry Team",
            "email": "dev@schooltry.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/console": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0",
        "neuron-core/neuron-ai": "^2.10"
    },
    "autoload": {
        "psr-4": {
            "SchoolTry\\AIDocumentationGenerator\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SchoolTry\\AIDocumentationGenerator\\Providers\\AIDocumentationServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}

