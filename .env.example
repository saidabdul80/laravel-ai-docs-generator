# AI Documentation Generator Configuration

# AI Provider (ollama, openai, claude, gemini)
AI_DOCS_PROVIDER=ollama

# Ollama Configuration
AI_DOCS_OLLAMA_URL=http://localhost:11434
AI_DOCS_OLLAMA_LIGHT_MODEL=qwen2.5:3b-instruct
AI_DOCS_OLLAMA_STANDARD_MODEL=qwen2.5:7b-instruct
AI_DOCS_OLLAMA_HEAVY_MODEL=qwen2.5:14b

# OpenAI Configuration
AI_DOCS_OPENAI_URL=https://api.openai.com/v1
AI_DOCS_OPENAI_KEY=
AI_DOCS_OPENAI_LIGHT_MODEL=gpt-3.5-turbo
AI_DOCS_OPENAI_STANDARD_MODEL=gpt-4
AI_DOCS_OPENAI_HEAVY_MODEL=gpt-4-turbo

# Claude Configuration
AI_DOCS_CLAUDE_URL=https://api.anthropic.com/v1
AI_DOCS_CLAUDE_KEY=
AI_DOCS_CLAUDE_LIGHT_MODEL=claude-3-haiku-20240307
AI_DOCS_CLAUDE_STANDARD_MODEL=claude-3-sonnet-20240229
AI_DOCS_CLAUDE_HEAVY_MODEL=claude-3-opus-20240229

# Gemini Configuration
AI_DOCS_GEMINI_URL=https://generativelanguage.googleapis.com/v1beta
AI_DOCS_GEMINI_KEY=
AI_DOCS_GEMINI_LIGHT_MODEL=gemini-1.5-flash
AI_DOCS_GEMINI_STANDARD_MODEL=gemini-1.5-pro
AI_DOCS_GEMINI_HEAVY_MODEL=gemini-2.0-flash

# Layout Files
AI_DOCS_ROUTER_FILE=resources/js/router/index.js
AI_DOCS_NAVIGATION_DIR=resources/js/components/Navigation_modules
AI_DOCS_LAYOUTS_DIR=resources/js/layouts

# Generation Settings
AI_DOCS_MAX_DEPTH=5
AI_DOCS_CHUNK_SIZE=2000
AI_DOCS_CONCURRENCY=1
AI_DOCS_MEMORY_LIMIT=40
AI_DOCS_OUTPUT_DIR=storage/app/ai_docs/frontend
AI_DOCS_CACHE_NAVIGATION=true

# Vector Database
AI_DOCS_VECTOR_DB_ENABLED=true
AI_DOCS_VECTOR_STORE_PATH=storage/app/ai_docs/frontend/neuron.store
AI_DOCS_VECTOR_CHUNK_SIZE=900
AI_DOCS_VECTOR_OVERLAP=200

# Route Links
AI_DOCS_ROUTE_LINKS_ENABLED=true
AI_DOCS_INCLUDE_RELATED_ROUTES=true
AI_DOCS_MAX_RELATED_ROUTES=5

# AI Service Upload
AI_DOCS_UPLOAD_ENABLED=false
AI_SERVICE_URL=http://localhost:8000
AI_SERVICE_API_KEY=
AI_DOCS_AGENT_NAME=documentation
AI_DOCS_UPLOAD_TIMEOUT=60
AI_DOCS_VERIFY_SSL=true

